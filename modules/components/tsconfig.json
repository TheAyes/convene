{
	"compilerOptions": {
		"allowJs": true,
		"target": "ES2017",
		"module": "esnext",
		"lib": [
			"es2020",
			"DOM"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "@builder.io/qwik",
		"strict": true,
		"declaration": true,
		"declarationDir": "lib-types",
		"resolveJsonModule": true,
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"sourceMap": true,
		//"noEmit": true,
		"declarationMap": true,
		"skipLibCheck": true,
		"incremental": true,
		"isolatedModules": true,
		"types": [
			"vite/client"
		],
		"rootDir": "src"
	},
	"include": [
		"src",
		"package.json"
	]
}
